create database test;
use test;
create table tbclass(id1 int, id2 int, id3 int);
show tables;
insert into tbclass values(1, 2, 3);
desc tbclass;
insert into tbclass values(1, 1, 4); 
desc tbclass;
alter table tbclass add primary key (id1, id2);
desc tbclass;
insert into tbclass values(1, 1, 5);
alter table tbclass drop primary key;
desc tbclass;
alter table tbclass add primary key(id1);
exit;