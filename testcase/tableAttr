create database test;
use database test;
create table tbclass(id int default 1, score float default 1.5, class varchar 4 default 'xxxx');
show tables;
create table tbtest(id int not null, name varchar, class varchar, score float not null, birthday date, primary key(id), foreign key(class) references tbclass(class));
show tables;
create table tbfail1(id int, id int);
create table tbfail2(id int, primary key(id1));
create table tbfail3(id int, foreign key(id1) references tbclass(id));
create table tbfail4(id int, foreign key(id) references tbclass(class));
create table tbfail5(id int, foreign key(id) references tbclass(id), foreign key(id) references tbtest(id));
create table tbfail6xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx(id int);
create table tbfail7(xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx varchar);
create table tbfail8(id int default 1.5);
create table tbfail9(id varchar 4 default 'xxxxx');
drop table tbtest;
exit;