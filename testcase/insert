CREATE DATABASE orderDB;
USE DATABASE orderDB;
CREATE TABLE nation (in INT NOT NULL, cn CHAR(100) NOT NULL, fn float NOT NULL, dn date NOT NULL, cc VARCHAR(25));
SHOW TABLES;
