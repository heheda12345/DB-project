create database test;
use test;
create table tbclass(id int not null default 1, score float not null default 1.5, class varchar(4) default 'xxxx');
show tables;
alter table tbclass add index idx1(id, score);
show tables;
alter table tbclass drop index idx1;
show tables;
alter table tbclass add index idx1(id);
show tables;
alter table tbclass add index fail1(id, class);
alter table tbclass add index idx1(id, score);
alter table tbclass add index fail2(id1);
alter table tbclass drop index idx2;
alter table tbclass drop index idx1;
show tables;
insert into tbclass values (2, 3.5, 'aaa');
desc tbclass;
insert into tbclass values (2, 3.5, 'bbb');
desc tbclass;
alter table tbclass add index idx1(score);
desc tbclass;
insert into tbclass values (2, 3.5, 'ccc');
desc tbclass;
alter table tbclass drop index idx1;
exit;